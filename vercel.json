{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/graph",
      "dest": "/api/graph.ts"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.ts"
    },
    {
      "src": "/api/graph-recent",
      "dest": "/api/graph-recent.ts"
    },
    {
      "src": "/api/stats-recent",
      "dest": "/api/stats-recent.ts"
    }
  ]
}

